<transform path=usr/share/icu/.+ -> drop>
<transform link path=.+/current -> drop>
# libicutest is used onbly for regression tests
<transform path=.+libicutest.so.* -> drop>

# by default everything is in library/icu package
<transform file link -> default pkg.type library-icu>
# utilities and their manpages are in developer/icu
<transform file link path=usr/bin/.* -> set pkg.type developer-icu>
<transform file link path=usr/sbin/.* -> set pkg.type developer-icu>
<transform file link path=usr/share/man/man1/.* -> set pkg.type developer-icu>
<transform file link path=usr/share/man/man8/.* -> set pkg.type developer-icu>
